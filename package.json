{
  "name": "headlines_next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run open-browser && next dev -p 9001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\" -c ./.eslintrc",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx}\" -c ./.prettierrc",
    "tscheck": "tsc --noEmit",
