{
  "name": "headlines_next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run open-browser && next dev -p 9001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,js,tsx,jsx}\" \"app/**/*.{ts,js,tsx,jsx}\" -c ./.eslintrc --max-warnings=0",
    "prettier:check": "prettier --check \"src/**/*.{ts,js,tsx,jsx}\" \"app/**/*.{ts,js,tsx,jsx}\" -c ./.prettierrc",
    "tscheck": "tsc --noEmit",
    "open-browser": "open http://localhost:9001",
    "check": "npm run prettier:check && npm run tscheck && npm run lint"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
